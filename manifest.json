{
    "manifest_version": 2,
    "name": "Tab Export",
    "version": "1.0.2",
    "description": "Allows for easy exporting of currently open tabs to a file.",
    "browser_action": {
        "browser_style": true,
        "default_title": "Tab Export",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "tabs"
    ]
}
